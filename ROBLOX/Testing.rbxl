						<string name="Name">FlagManager</string>
						<ProtectedString name="Source">teams = game.Teams:children()
owner = script.Parent.CurrentOwner
neutral = script.Parent.NeutralColor.Value
a = script.Parent.Flag
b = script.Parent.FlagDetail
namer = script.Parent.Naming:children()[1]
steps = script.Parent.StepsToBottom
maxsteps = steps.Value

function setup(color)
local found = false
for i,v in pairs(teams) do
if v.TeamColor == color and v.TeamColor ~= neutral then
found = true
a.BrickColor = color
b.BrickColor = color
namer.Name = &quot;Owned by &quot; ..v.Name
break
end
end
if not found and color == neutral then
a.BrickColor = color
b.BrickColor = color
namer.Name = &quot;No one owns this flag!&quot;
a.CFrame = a.CFrame-Vector3.new(0,(steps.Value/maxsteps)*14,0)
b.CFrame = b.CFrame-Vector3.new(0,(steps.Value/maxsteps)*14,0)
steps.Value = 0
elseif not found and color ~= neutral then
print(&quot;Flag has no identity! Self-Destructing.&quot;)
script.Parent:Remove()
end
end


setup(owner.Value)

owner.Changed:connect(function() setup(owner.Value) end)</ProtectedString>
