			<Item class="Script" referent="RBX94">
				<Properties>
					<bool name="Disabled">false</bool>
					<Content name="LinkedSource"><null></null></Content>
					<string name="Name">FlagTester</string>
					<ProtectedString name="Source">-- EDITED FOR DOMINATION MODE
if(game.Workspace:findFirstChild(&quot;TimeMessage&quot;)~=nil) then
game.Workspace.WinMessage:Destroy()
end
if(game.Workspace:findFirstChild(&quot;WinMessage&quot;)~=nil) then
game.Workspace.WinMessage:Destroy()
end

print(&quot;Flag Tester Loaded.&quot;)

-- You should be able to add more flags without a problem.

flags = 
{
[script.Parent:findFirstChild(&quot;Outpost 1&quot;)] = 0,
[script.Parent:findFirstChild(&quot;Outpost 2&quot;)] = 0, 
[script.Parent:findFirstChild(&quot;Xerz&quot;)] = 0
}

h = Instance.new(&quot;Hint&quot;,workspace)
h.Name = &quot;TimeMessage&quot;
m = Instance.new(&quot;Message&quot;,workspace)
m.Name = &quot;WinMessage&quot;

TeamA = &quot;NTPC&quot;
ColorA = &quot;Really black&quot;
TeamB = &quot;Raiders&quot;
ColorB = &quot;Bright yellow&quot;

wintime = 1800
atime = 0
btime = 0
ENDGAME = false -- basically if win conditions are met it stops the script


function Update(cond)
&#9;if cond == 0 then
&#9;&#9;h.Text = (&quot;&quot; ..TeamA.. &quot;: &quot; ..atime.. &quot;/&quot; ..wintime.. &quot;. &quot; ..TeamB.. &quot;: &quot; ..btime.. &quot;/&quot; ..wintime.. &quot;.&quot;)
&#9;elseif cond == 1 then
&#9;&#9;h.Text = (&quot;&quot; ..TeamA .. &quot; has proven dominance!&quot;)
&#9;&#9;ENDGAME = true
&#9;&#9;m.Text = (&quot;&quot; ..TeamA .. &quot; has proven dominance!&quot;)
&#9;&#9;wait(15)
&#9;&#9;atime = 0
&#9;&#9;btime = 0
&#9;&#9;ENDGAME = false
&#9;&#9;m.Text = &quot;&quot;
&#9;elseif cond == 2 then
&#9;&#9;h.Text = (&quot;&quot; ..TeamB .. &quot; has proven dominance!&quot;)
&#9;&#9;ENDGAME = true
&#9;&#9;m.Text = (&quot;&quot; ..TeamB .. &quot; has proven dominance!&quot;)
&#9;&#9;wait(15)
&#9;&#9;atime = 0
&#9;&#9;btime = 0
&#9;&#9;ENDGAME = false
&#9;&#9;m.Text = &quot;&quot;
&#9;end
end

while true do
while ENDGAME == false do
&#9;wait(1)
&#9;isaraid = false -- initially, there is no raid; it has to start (see next comment)

&#9;local p = game.Players:GetChildren()
&#9;for i=1,#p do -- checks if there is a raid
&#9;&#9;if p[i].TeamColor == BrickColor.new(ColorB) then isaraid = true end
&#9;end
&#9;if isaraid==true then -- will add to the defense timer when a raider is in the server
&#9;&#9;for k, v in pairs(flags) do
&#9;&#9;&#9;if(k.CurrentOwner.Value == BrickColor.new(ColorA)) then
&#9;&#9;&#9;&#9;if(k.Name==&quot;Xerz&quot;) then atime = atime + 2
&#9;&#9;&#9;&#9;else atime = atime + 1 end
&#9;&#9;&#9;elseif (k.CurrentOwner.Value == BrickColor.new(ColorB)) then
&#9;&#9;&#9;&#9;if(k.Name==&quot;Xerz&quot;) then btime = btime + 2
&#9;&#9;&#9;&#9;else btime = btime + 1 end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;

&#9;Update(0) -- updates hint at the top of the screen
&#9;if atime &gt;= wintime then Update(2) -- defender win
&#9;elseif btime &gt;= wintime then Update(1) -- raider win
&#9;end
end
wait(1)
end
